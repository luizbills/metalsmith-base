{
  "destination": "_site",
  "plugins": {
    "metalsmith-ignore": {
      "patterns": [
        "**/_*",
        "_**/**/*",
        "**/_**/*",
        "**/_**/**/*"
      ]
    },
    "metalsmith-nunjucks": {
      "pattern": "*.html",
      "directory": "_templates"
    },
    "metalsmith-cssnext": {
    }
  }
}